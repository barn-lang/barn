@import "format.ba"
@import "std.ba"

fun test_printf(string __format_start__, ...) {
	__barn_start_format()

	// We expect that the first format is string
	println(__format_start__)
	
	let get_first_format: cstr = __barn_format_get_value_cstr()
	println(get_first_format)
	get_first_format = __barn_format_get_value_cstr()
	println(get_first_format)
}

fun main() {
	test_printf("Hello %s", "World", "ahahH")
}
